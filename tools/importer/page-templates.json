{
  "templates": [
    {
      "name": "wknd-trendsetters-homepage",
      "urls": ["https://wknd-trendsetters.site"],
      "description": "Fashion blog homepage template with hero, features grid, articles, tabs showcase, FAQ accordion, and CTA sections",
      "blocks": [
        {
          "name": "section-hero",
          "section": true,
          "selector": "header.section.secondary-section",
          "sectionMetadata": null,
          "content": [
            {
              "name": "default-content",
              "selector": ".container.small-container",
              "description": "Hero heading, subheading, and CTA buttons"
            },
            {
              "name": "columns-layout",
              "selector": ".grid-layout.mobile-portrait-1-column.grid-gap-md",
              "description": "Two side-by-side hero images"
            }
          ]
        },
        {
          "name": "section-features",
          "section": true,
          "selector": "section.section:has(h2:contains('Trends made'))",
          "sectionMetadata": null,
          "content": [
            {
              "name": "default-content",
              "selector": "h2.utility-text-align-center",
              "description": "Section heading"
            },
            {
              "name": "cards-features",
              "selector": ".grid-layout.desktop-4-column.tablet-3-column",
              "description": "8-item features grid with icons and text"
            },
            {
              "name": "default-content",
              "selector": ".button-group.align-center",
              "description": "CTA buttons"
            }
          ]
        },
        {
          "name": "section-articles",
          "section": true,
          "selector": "section.section.secondary-section:has(.eyebrow)",
          "sectionMetadata": {
            "style": "secondary"
          },
          "content": [
            {
              "name": "columns-layout",
              "selector": ".grid-layout.mobile-landscape-1-column.grid-gap-xs",
              "description": "Eyebrow, heading, text, and browse button"
            },
            {
              "name": "cards-articles",
              "selector": ".grid-layout.tablet-1-column.grid-gap-md",
              "description": "4 article cards with images"
            }
          ]
        },
        {
          "name": "section-tabs",
          "section": true,
          "selector": "section.section.inverse-section",
          "sectionMetadata": {
            "style": "dark"
          },
          "content": [
            {
              "name": "tabs-showcase",
              "selector": ".w-tabs",
              "description": "3-tab navigation with images"
            }
          ]
        },
        {
          "name": "section-faq",
          "section": true,
          "selector": "section.section.secondary-section:has(h2:contains('Style Qs'))",
          "sectionMetadata": {
            "style": "secondary"
          },
          "content": [
            {
              "name": "default-content",
              "selector": ".utility-margin-bottom-4rem.utility-text-align-center",
              "description": "FAQ heading and subheading"
            },
            {
              "name": "accordion-faq",
              "selector": ".flex-vertical:has(.accordion)",
              "description": "4 FAQ accordion items"
            }
          ]
        },
        {
          "name": "section-cta",
          "section": true,
          "selector": "section.section:has(h2:contains('Join the style'))",
          "sectionMetadata": null,
          "content": [
            {
              "name": "columns-layout",
              "selector": ".grid-layout.desktop-4-column.tablet-1-column.grid-gap-sm",
              "description": "CTA with heading, text, and buttons"
            }
          ]
        }
      ]
    }
  ]
}
